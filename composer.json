{
    "name": "hnhdigital-os/laravel-search-components",
    "description": "Search view components.",
    "keywords": ["laravel"],
    "license": "MIT",
    "authors": [
        {
            "name": "Rocco Howard",
            "email": "rocco@hnh.digital"
        }
    ],
    "require": {
        "php": "^8.0.2",
        "laravel/framework": "^9.0",
        "hnhdigital-os/laravel-html-generator": "^3.0",
        "hnhdigital-os/laravel-html-builder": "^3.0",
        "hnhdigital-os/laravel-model-search": "^3.0"
    },
    "require-dev": {

    },
    "autoload": {
        "psr-4": {
            "HnhDigital\\SearchComponents\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "HnhDigital\\SearchComponents\\ServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": [
            "composer phpunit && composer phpcs && composer phpmd"
        ],
        "phpunit": [
            "phpunit"
        ],
        "phpcs": [
            "phpcs -p -s --standard=PSR2 ./src ./tests"
        ],
        "phpmd": [
            "phpmd ./src phpmd.xml"
        ]
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "process-timeout": 2000,
        "sort-packages": true,
        "platform": {
            "php": "7.3.0"
        }
    },
    "prefer-stable": true,
    "minimum-stability" : "dev"
}
